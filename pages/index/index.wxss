/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaa;
}
.usermotto {
  margin-top: 200px;
}
text {
  color: var(--themeColor);
}
image {
  width: 100%;
}
swiper {
  width: 100%;
  height: 340rpx;
}
.navcate {
  display: flex;
  width: 100%;
  height: 150rpx;
}
.navcate_item {
  flex: 1;
  margin: 10rpx;
  background-color: #fff;
  text-align: center;
}
.navcate_item image {
  width: 128rpx;
}
.floor {
  margin-top: 30rpx;
}
.floor .floorHeader {
  height: 50rpx;
}
.floor .floorBody {
  display: flex;
  padding: 20rpx;
}
.floor .floorBody .leftBigOne {
  width: 232rpx;
  height: 386rpx;
}
.floor .floorBody .four {
  display: flex;
  flex: 1;
  flex-wrap: wrap;
  padding: 0 10rpx;
}
.floor .floorBody .four view {
  padding: 5rpx;
  box-sizing: border-box;
  width: 50%;
}
.floor .floorBody .four view image {
  width: 100%;
  height: 100%;
}
